    <validationRules>
        <fullName>Duns_Number_Validation</fullName>
        <active>true</active>
        <errorConditionFormula>NOT(OR(ISBLANK(DUNS_Custom__c),
 REGEX(DUNS_Custom__c, &quot;[0-9]{2}-[0-9]{3}-[0-9]{4}&quot;)))</errorConditionFormula>
        <errorMessage>Company D-U-N-S Number needs to be in 99-999-9999 format.</errorMessage>
    </validationRules>
